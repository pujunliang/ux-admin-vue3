        "@vitejs/plugin-vue": "^4.2.3",
        "@vitejs/plugin-vue-jsx": "^3.0.2",
        "consola": "^3.2.3",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-vue": "^9.17.0",
        "mockjs": "^1.1.0",
        "prettier": "^3.0.3",
        "rollup-plugin-visualizer": "^5.9.2",
